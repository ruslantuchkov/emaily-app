{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "8.7.0",
    "npm": "5.6.0"
  },
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\" \"yarn run webhook\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "webhook": "lt -s gwerdfjku --port 5000"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.7",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^3.0.1",
    "sendgrid": "^5.2.3",
    "stripe": "^5.5.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "localtunnel": "^1.8.3",
    "nodemon": "^1.17.1"
  }
}
